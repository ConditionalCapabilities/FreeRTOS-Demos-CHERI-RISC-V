top = '.'
out = 'build'

def configure(ctx):
    print("Configuring CWE-457 Demo @", ctx.path.abspath())
    
    ctx.env.append_value(
        'INCLUDES',
        [
            ctx.path.abspath() + "/",
            ctx.path.abspath() + "/freertos/",
            ctx.path.abspath() + "/testcasesupport/",
        ])

    ctx.env.append_value('DEFINES', [
        'configPROG_ENTRY     = main',
        'configCHERI_INT_MEMCPY = 0'
    ])



    ctx.define('configCHERI_VERBOSE_FAULT_INFO', 1)
    # ctx.define('configCHERI_STACK_TRACE', 1)
    ctx.env.LIBDL_PROG_START_FILE = "main_CWE457_Use_of_Uninitialized_Variable.c"
    juliet_mode_value = int(ctx.env.JUL_MODE[1:])

    # Check if the mode value is within the supported range.
    if 0 <= juliet_mode_value <= 126:
        ctx.define(f'configJUL_MODE', juliet_mode_value)
    else:
        ctx.fatal('Invalid compartmentalization mode: only versions 1 to 109 are supported')
    

def build(bld):
    name = "main_CWE457_Use_of_Uninitialized_Variable"
    print("Building CWE457_Use_of_Uninitialized_Variable")

    cflags = []
    
    #if ctx.env.opbounds_simple:
    #   cflags.append('-Werror=cheri-prototypes')


    if bld.env.COMPARTMENTALIZE and bld.env.PURECAP:
        cflags = ['-cheri-cap-table-abi=gprel']

    if bld.env.COMPARTMENTALIZE and bld.env.ENABLE_MPU:
        # To force emitting relocs for function pointers
        cflags = ['-mcmodel=medlow']

    cflags.append('-Werror=cheri-prototypes')
    cflags.append('-Werror=cheri-capability-misuse')
    cflags.append('-Werror=cheri-provenance')
    cflags.append('-Werror=cheri-inefficient')

    if bld.env.OPBOUNDS_SIMPLE_MODE:
        cflags.append('-mllvm')
        cflags.append('-cheri-writebeforeread-opt=simple')

    # ldlags=['-lCWE457_Use_of_Uninitialized_Variable']
    
    bld.stlib(
        features=['c'],
        cflags=bld.env.CFLAGS + cflags,
        source=[
            '/../testcasesupport/io.c', 
            '/../testcasesupport/PvPortMalloc.c',
